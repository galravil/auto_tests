{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "multiple articles",
  "type": "object",
  "properties": {
      "articles": {
          "type": "array",
          "properties": {
              "author": {
                  "type": "object",
                  "properties": {
                      "username": {
                          "type": "string"
                      },
                      "bio": {
                          "type": "string"
                      },
                      "image": {
                          "type": "string"
                      },
                      "following": {
                          "type": "boolean"
                      }
                  }
              },
              "slug": {
                  "type": "string"
              },
              "title": {
                  "type": "string"
              },
              "description": {
                  "type": "string"
              },
              "body": {
                  "type": "string"
              },
              "tagList": {
                  "type": "array"
              },
              "createdAt": {
                  "type": "string"
              },
              "updatedAt": {
                  "type": "string"
              },
              "favorited": {
                  "type": "boolean"
              },
              "favoritesCount": {
                  "type": "number"
              }
          }
      },
      "articlesCount": {
          "type": "number"
      }
  }
}